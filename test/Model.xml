<?xml version="1.0" encoding="utf-8"?>
<Tables xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:schemaLocation="http://www.newlifex.com http://www.newlifex.com/Model2020.xsd" NameSpace="Crops.Modules.BasicData" ConnName="Crops" Output="" BaseClass="Entity" xmlns="http://www.newlifex.com/Model2020.xsd">
  <Table Name="Tenant" Description="租户信息">
    <Columns>
      <Column Name="Id" DataType="Int32" Identity="True" PrimaryKey="True" Description="编号" />
      <Column Name="TenantCode" DataType="String" Nullable="False" Description="租户编号" />
      <Column Name="TenantName" DataType="String" Master="True" Nullable="False" Description="租户名称" />
      <Column Name="TenantType" DataType="String" Description="租户类型" Attribute="CategoryModule(nameof(TenantType))" />
      <Column Name="BusinessLicense" DataType="String" Description="营业许可证" />
      <Column Name="BdentityCard" DataType="String" Description="身份证号" />
      <Column Name="Phone" DataType="String" Nullable="False" Description="手机号" />
      <Column Name="Email" DataType="String" Description="电子邮箱" />
      <Column Name="BankNo" DataType="String" Description="银行卡号" />
      <Column Name="BankName" DataType="String" Description="银行名称" />
      <Column Name="CreateUser" DataType="String" Description="创建者" />
      <Column Name="CreateIP" DataType="String" Description="创建地址" />
      <Column Name="CreateTime" DataType="DateTime" Description="创建时间" />
      <Column Name="UpdateUser" DataType="String" Description="更新者" />
      <Column Name="UpdateIP" DataType="String" Description="更新地址" />
      <Column Name="UpdateTime" DataType="DateTime" Description="更新时间" />
      <Column Name="Remark" DataType="String" Length="500" Description="备注" />
    </Columns>
  </Table>
  <Table Name="BusinessType" Description="品牌业态">
    <Columns>
      <Column Name="Id" DataType="Int32" Identity="True" PrimaryKey="True" Description="编号" />
      <Column Name="BusinessCode" DataType="String" Nullable="False" Description="业态代码" />
      <Column Name="BusinessName" DataType="String" Nullable="False" Description="业态名称" />
      <Column Name="ParentId" DataType="Int32" Description="上级代码" />
    </Columns>
    <Indexes>
      <Index Name="BusinessCode_pk" Columns="BusinessCode" Unique="True" />
    </Indexes>
  </Table>
  <Table Name="Brand" Description="品牌信息">
    <Columns>
      <Column Name="Id" DataType="Int32" Identity="True" PrimaryKey="True" Description="编号" />
      <Column Name="BrandCode" DataType="String" Nullable="False" Description="品牌代码" />
      <Column Name="BrandName" DataType="String" Master="True" Nullable="False" Description="品牌名称" />
      <Column Name="BrandEnName" DataType="String" Description="品牌英文名称" />
      <Column Name="Business" DataType="Int32" Description="一级业态" />
      <Column Name="BusinessSub" DataType="Int32" Description="二级级业态" />
      <Column Name="CreateUser" DataType="String" Description="创建者" />
      <Column Name="CreateUserID" DataType="Int32" Description="创建人" />
      <Column Name="CreateIP" DataType="String" Description="创建地址" />
      <Column Name="CreateTime" DataType="DateTime" Description="创建时间" />
      <Column Name="UpdateUser" DataType="String" Description="更新者" />
      <Column Name="UpdateUserID" DataType="Int32" Description="更新人" />
      <Column Name="UpdateIP" DataType="String" Description="更新地址" />
      <Column Name="UpdateTime" DataType="DateTime" Description="更新时间" />
      <Column Name="Remark" DataType="String" Length="500" Description="备注" />
    </Columns>
  </Table>
</Tables>